<?xml version="1.0" encoding="UTF-8"?>
<!--NEMESIS mainwindow setup-->
<desktop>
    <mainwindow>
        <toolbars/>
        <shortcuts>
            <shortcut action="actionNewMolecule" key="Ctrl+N"/>
            <shortcut action="actionOpen" key="Ctrl+O"/>
            <shortcut action="actionSave" key="Ctrl+S"/>
            <shortcut action="actionNewProject" key="Ctrl+P"/>
            <shortcut action="actionSaveAll" key="Ctrl+L"/>
            <shortcut action="actionUndo" key="Ctrl+Z"/>
            <shortcut action="actionRedo" key="Ctrl+Shift+Z"/>
            <shortcut action="actionCutStructure" key="Ctrl+X"/>
            <shortcut action="actionPasteStructure" key="Ctrl+V"/>
            <shortcut action="actionCopyStructure" key="Ctrl+C"/>
            <shortcut action="actionClean" key="A"/>
            <shortcut action="actionZoomIn" key="Num++"/>
            <shortcut action="actionZoomOut" key="Num+-"/>
            <shortcut action="actionSelectAll" key="Ctrl+A"/>
        </shortcuts>
    </mainwindow>
</desktop>
